//Update Scrolls and Invoke Items Targeting 
// Update Descriptions 
COPY_EXISTING_REGEXP GLOB ~.+\.itm~ ~override~
	PATCH_IF (~%SOURCE_EXT%~ STRING_EQUAL_CASE ~itm~) BEGIN 
		SPRINT item_res ~%SOURCE_RES%~ 
		READ_SHORT 0x1c item_type 
		READ_STRREF 0x54 itm_desc

		READ_LONG  0x64 ofs_headers
		READ_SHORT 0x68 num_headers 
		READ_LONG  0x6a ofs_eff

		SET teaches = 0 
		SET invokes = 0 
		
		PATCH_IF (num_headers > 0) BEGIN 
			// iterate over headers 
			FOR (i=0; i < num_headers; ++i) BEGIN
				READ_SHORT (ofs_headers + i*0x38 + 0x1e) num_eff 
				READ_SHORT (ofs_headers + i*0x38 + 0x20) ind_eff 

				//iterate over effects of each header 
				FOR (j=ind_eff; j < ind_eff+num_eff; ++j) BEGIN
					READ_SHORT (ofs_eff + j*0x30 + 0x00) eff_opcode 

					// identify location of cast spell effects
					PATCH_IF ((eff_opcode = 146) OR (eff_opcode = 148)) BEGIN
						SET invokes = 1 
						//identify spell that is being cast 
						READ_ASCII (ofs_eff + j*0x30 + 0x14) spell 
						PATCH_IF (FILE_EXISTS_IN_GAME ~%spell%.spl~) BEGIN 
							INNER_ACTION BEGIN 	
								COPY_EXISTING ~%spell%.spl~ ~override~ 
									READ_STRREF 0x08 spl_name 
									READ_STRREF 0x08 spl_name_upper
									READ_SHORT  0x1c spl_type
 									TO_UPPER spl_name_upper 
									READ_LONG 0x64 abil_offset
									READ_BYTE	(abil_offset + 0xc) spl_target
									READ_SHORT  (abil_offset + 0xe) spl_range
									READ_SHORT  (abil_offset + 0x10) spl_cast_level
									READ_STRREF 0x50 spl_desc
								BUT_ONLY
							END 
						END 
						//Update targeting and range 
						WRITE_BYTE (ofs_headers + i*0x38 + 0x0c) spl_target
						WRITE_SHORT (ofs_headers + i*0x38 + 0x0e) spl_range

					
						WRITE_LONG (ofs_eff + j*0x30 + 0x04) spl_cast_level 

						//Update cast opcode for point vs. actor targeting
						PATCH_IF (spl_target = 4) BEGIN 
							WRITE_SHORT (ofs_eff + j*0x30 + 0x00) 148 
							WRITE_SHORT (ofs_eff + j*0x30 + 0x02) 1 
						END ELSE BEGIN 
							WRITE_SHORT (ofs_eff + j*0x30 + 0x00) 146 
							WRITE_SHORT (ofs_eff + j*0x30 + 0x02) 2
						END 
					END
					PATCH_IF (eff_opcode = 147) BEGIN
						SET teaches = 1  
						//identify spell that is being taught 
						READ_ASCII (ofs_eff + j*0x30 + 0x14) spell 
						PATCH_IF (FILE_EXISTS_IN_GAME ~%spell%.spl~) BEGIN 
							INNER_ACTION BEGIN 	
								COPY_EXISTING ~%spell%.spl~ ~override~ 
									READ_STRREF 0x08 spl_name 
									READ_STRREF 0x08 spl_name_upper
									READ_SHORT  0x1c spl_type
 									TO_UPPER spl_name_upper 
									READ_LONG 0x64 abil_offset
									READ_BYTE	(abil_offset + 0xc) spl_target
									READ_SHORT  (abil_offset + 0xe) spl_range
									READ_SHORT  (abil_offset + 0x10) spl_cast_level
									READ_STRREF 0x50 spl_desc
								BUT_ONLY
							END 
						END 
					END 
				END 
			END 
		END 

		// Update Scroll Descriptions (matching itm/spl res and type 11)
		PATCH_IF ((~%item_res%~ STRING_EQUAL_CASE ~%spell%~) AND (item_type = 11)) BEGIN 
			//mage scrolls teach and invoke 
			PATCH_IF (spl_type = 1) BEGIN 
				SAY NAME2 @5000
				SAY IDENTIFIED_DESC @5001
			END
			//priest scrolls don't teach
			PATCH_IF (spl_type = 2) BEGIN 
				SAY NAME2 @5000
				SAY IDENTIFIED_DESC @5002 
			END 
		END ELSE BEGIN 
			//Add spell info to other items 
			//Updated items base descriptions prior to this function 
			PATCH_IF ((teaches = 1) OR (invokes = 1)) BEGIN 
				SAY IDENTIFIED_DESC @5009
			END 
		END  

	END 
BUT_ONLY
