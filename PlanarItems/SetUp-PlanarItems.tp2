//handling the variable checks for special tattoos
COPY_EXISTING ~fell.sto~ ~override~
	GET_OFFSET_ARRAY tattoos 0x34 4 0x38 4 0 0 0x58 
	PHP_EACH tattoos AS int => tt_off BEGIN 
		READ_ASCII tt_off itmres
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttdak~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~InParty("Dakkon")~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttvhl~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~InParty("Vhail")~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~ttanarch~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~Faction(Protagonist,ANARCHIST)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~ttsource~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~Faction(Protagonist,GODSMAN)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~ttsens1~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~Faction(Protagonist,SENSATE)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttdst~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~Faction(Protagonist,DUSTMEN)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttxao~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~Faction(Protagonist,XAOSITECT)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttdas~) BEGIN 
			WRITE_LONG (tt_off + 0x1c) RESOLVE_STR_REF(~GlobalGT("RW", "GLOBAL", 2)~)
		END 
		PATCH_IF (~%itmres%~ STRING_EQUAL_CASE ~qdttbrs~) BEGIN 
