BACKUP ~PlanarItems/backup~
AUTHOR ~aquadrizzt@gmail.com~
VERSION ~0.6~ 

/* Language Settings */
AUTO_TRA ~PlanarItems/tra/%s~
LANGUAGE ~English~ ~english~ ~PlanarItems/tra/english/setup.tra~

//---------------------------------------------
//                                             
//				MAIN ITEM REWORK                
//                                             
//---------------------------------------------
BEGIN @0 

COPY_EXISTING ~aclue.itm~ ~override/qd_planar_items_core.qd~ //check file for this component 

//---------------------------------------------                                        
//				BASE CHANGES                                 
//---------------------------------------------
//Small UI change to access item abilities from equipment 
//Thanks kjeron! 
COPY_EXISTING ~ui.menu~ ~override~ 
	REPLACE_TEXTUALLY ~showContextMenu(const.ACTIONBAR_STATE_ITEMS)~ "if	contextBarState ~= const.ACTIONBAR_STATE_ITEMS then
		showContextMenu(const.ACTIONBAR_STATE_ITEMS)
	else
		showContextMenu(105)
	end"
BUT_ONLY

//TNO gets a +2 bonus to AC after character creation 
COPY_EXISTING ~charbase.cre~ ~override~ 
	LPF ADD_CRE_EFFECT 
		INT_VAR 
			target = 1 
			timing = 9
			parameter1 = 2 
	END 

//Updated Grace's natural AC to 10 
//This fixes an issue where the new armor gives her a base AC of -2 
COPY_EXISTING ~grace.cre~ ~override~ 
	WRITE_SHORT 0x46 10 
	WRITE_SHORT 0x48 10 

//Added the summoning limit 2da from SoD in order to allow invisible creature tricks 
COPY ~PlanarItems/resources/SUMMLIMT.2da~ ~override~ 

//Monsters with immunity to weapons below an enchantment level are now considered to have enchanted weapons. 
COPY ~PlanarItems/items/PLUS1IM.itm~ ~override~ 
	 ~PlanarItems/items/PLUS2IM.itm~ ~override~ 

//Change default body "armour" sound to tattoo <this is for major tattoos>
//Annah and Grace armor should be unaffected
COPY_EXISTING ~itemtype.2da~ ~override~ 
	SET_2DA_ENTRY 3 1 3 ~INT_12K~ 
 	SET_2DA_ENTRY 3 2 3 ~INT_12K1~ 	 

// Change "Hammer" proficiency to now be "Blunt" proficiency
// Change "Edged Weapon" proficiency to now be "Edged" proficiency
COPY_EXISTING ~WEAPPROF.2da~ ~override~ 
	SET_2DA_ENTRY 4 2 5 RESOLVE_STR_REF(@0100) 
	SET_2DA_ENTRY 3 2 5 RESOLVE_STR_REF(@0101) 
	PRETTY_PRINT_2DA

// Replace references to club proficiency with hammer proficiency
// [To-Do: Improved this later, e.g. removing unnecessary dialog states]
COPY_EXISTING_REGEXP GLOB ~.+\.dlg~ ~override~
	DECOMPILE_DLG_TO_D
        REPLACE_TEXTUALLY CASE_INSENSITIVE EXACT_MATCH ~Protagonist,CLUB~ ~Protagonist,HAMMER~
    COMPILE_D_TO_DLG
    BUT_ONLY 

//Add immunity to maze to the NO_CURSE.itm that bosses have (for Tattoo of the Devourer)
//COPY ~PlanarItems/items/NO_CURSE.itm~ ~override~ 

//Update PROJECTL.ids 
COPY ~PlanarItems/resources/PROJECTL.ids~ ~override~ 

//Aura Projectiles

//Allies 60 Ft Radius (e.g. Blessing)  
ADD_PROJECTILE ~PlanarItems/resources/qd60ftg.pro~ ~override~ //invisible, 60 ft radius, allies only <affects some select, random npcs>

//Enemies 60 Ft Radius (e.g. Curse) 
ADD_PROJECTILE ~PlanarItems/resources/qd60ftb.pro~ ~override~ //invisible, 60 ft radius, enemies only 

